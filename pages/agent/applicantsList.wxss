/* pages/agent/applicantsList.wxss */
.container{
    width: 100vw;
    height: 100vh;
    background: #fff;
}

.list{
    width: 100vw;
    height: 168rpx;
    display: flex;
    align-items: center;
}

.avatar{
    width: 80rpx;
    height: 80rpx;
    margin-left: 32rpx;
}

.content{
    display: flex;
    flex-direction: column;
    margin-left: 16rpx;
    height: 84rpx;
    justify-content: space-between;
}

.nick-name{
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFangSC;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:44rpx;
}

.phone{
    font-size:23rpx;
    font-family:SFProDisplay-Regular,SFProDisplay;
    font-weight:400;
    color:rgba(184,184,184,1);
    line-height:30rpx;
}